pipeline {
    agent any
    stages {
        stage ('clean-up') {
            steps {
                cleanWs()
            }
        }
        
        
        stage ('build') {
            
                steps {
                 sh 'mvn clean install'
            }
            
        }
        
        stage ('test') {
            
                steps {
                    sh 'mvn clean test'
                    archiveArtifacts artifacts: 'target/*.war', followSymlinks: false
                
            }
        }
    }
}
